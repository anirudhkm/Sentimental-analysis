CssClasses":[["includes-citation-list"]],"isFulltext":true,"isSlurp":false,"isNoText":false,"publicationType":"Article","publicationDate":"Apr 2016","journal":"Journal of Experimental Psychology General","showEnrichedPublicationItem":false,"citationCount":0,"commentCount":1,"requestFulltextButton":null,"publicationUrl":"publication\/299859911_Semantic_prosody_and_judgment","usePlainButton":true,"publicationUid":299859911,"feedStubs":null,"hasFeedStubs":false,"impactPoints":"5.50","url":"publication\/299859911_Semantic_prosody_and_judgment","title":"Semantic prosody and judgment","displayTitleAsLink":true,"authors":[{"id":2040701837,"url":"researcher\/2040701837_David_Hauser","fullname":"David Hauser","last":false,"imageUrl":"https:\/\/i1.rgstatic.net\/ii\/profile.image\/AS%3A348248872112129%401460040409621_m\/David_Hauser2.png"},{"id":75137,"url":"researcher\/75137_Norbert_Schwarz","fullname":"Norbert Schwarz","last":true,"imageUrl":"https:\/\/i1.rgstatic.net\/ii\/profile.image\/AS%3A272390324224015%401441954322904_m\/Norbert_Schwarz2.png"}],"displayAuthorsAsLinks":true,"displayAuthors":true,"displayJournalMetadata":true,"collapseTitle":false,"collapseAuthors":false,"collapseAbstract":false,"details":["Journal of Experimental Psychology General 04\/2016;"],"abstract":"Some words tend to co-occur exclusively with a positive or negative context in natural language use even though such valence patterns are not dictated by definitions or are part of the words\u2019 core meaning. These words contain semantic prosody, a subtle valenced meaning derived from co-occurrence in language. As language and thought are heavily intertwined, we hypothesized that semantic prosody can affect evaluative inferences about related ambiguous concepts. Participants inferred that an ambiguous medical outcome was more negative when it was caused, a verb with negative semantic prosody, than when it was produced, a synonymous verb with no semantic prosody (Studies 1a, 1b). Participants completed sentence fragments in a manner consistent with semantic prosody (Study 2), and semantic prosody affected various other judgments in line with evaluative inferences (estimates of an event\u2019s likelihood in Study 3). Finally, semantic prosody elicited both positive and negative evaluations of outcomes across a large set of semantically prosodic verbs (Study 4). Thus, semantic prosody can exert a strong influence on evaluative judgment.","description":false,"swapJournalAndAuthorPositions":false,"showAbstract":true,"type":"Article","showType":true,"showPublicationIcon":false,"publicationUrlWithContext":"publication\/299859911_Semantic_prosody_and_judgment","isReader":null,"isAuthor":null,"isRequester":null,"hasFulltext":true,"actions":[{"type":"download","text":"Download","url":"profile\/David_Hauser2\/publication\/299859911_Semantic_prosody_and_judgment\/links\/570a788608ae2eb9421fbda2.pdf?origin=publication_list","active":false,"primary":true,"extraClass":"open-viewer","icon":null,"data":[]}],"actionWidgets":[],"publicationItemFulltext":{"data":{"isDataset":false,"isResearch":false,"isFulltext":true,"source":{"isPreview":false,"sourceUrl":"profile\/David_Hauser2","sourceName":"David Hauser","hasSourceUrl":true},"publicationUid":299859911,"publicationUrl":"publication\/299859911_Semantic_prosody_and_judgment","eventCode":"","citationCount":0,"additionalContentWidgets":[],"disableViewer":true,"showLoggedOutRequestButton":false,"context":null,"previewUrl":"https:\/\/i1.rgstatic.net\/publication\/299859911_Semantic_prosody_and_judgment\/links\/570a788608ae2eb9421fbda2\/smallpreview.png","linkId":"570a788608ae2eb9421fbda2","origin":"publication_list","showRequestCount":false,"publish":false,"request":false,"showAction":false,"displayAsLink":true,"preloadViewer":true,"documentViewerUrl":"publicliterature.PublicLiteratureDocumentViewer.html?publicationId=299859911&reference=570a788608ae2eb9421fbda2&eventCode=&origin=publication_list","widgetId":"rgw40_572ab339d26ea"},"i